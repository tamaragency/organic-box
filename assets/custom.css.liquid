.template--page-questionnaire .step-navigation--item .progress-bar{
  background: {{ text-color-green }};
  color:#fff;
}
.template--page-questionnaire .step-navigation--item{
  position:relative;
}
.template--page-questionnaire .step-navigation--item:before{
  background: {{ text-color-white }};
  position: absolute;
  bottom: 0;
  left: 20px;
  width: calc(100% - 40px);
  height: 4px;
  content: "";
  margin-right: 40px;
  z-index:1;
}
.template--page-questionnaire .step-navigation--item .progress-bar{
  height: 4px;
  margin-top: 16px;

.template--page-questionnaire .step-navigation--item .heading-number{
  border-radius:50%;
  -webkit-border-radius:50%;
  -moz-border-radius:50%;
  -o-border-radius:50%;
  background:{{ text-color-white }};
  width:{{ heading-number-circle-size }};
  height:{{ heading-number-circle-size }};
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  align-items:center;

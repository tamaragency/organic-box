.color-palette__consultation-overlay {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 999;
  background-color: rgba(255, 255, 255, .7);
  opacity: 0;
  visibility: hidden;
  overflow: scroll;
}

.color-palette__consultation-overlay.active {
  opacity: 1;
  visibility: visible;

}
  
.color-palette__consultation-overlay__container {
  position: relative;
  background-color: #fff;
  box-shadow: 0px 0px 15px gray;
  max-width: 767px;
  width: 100%;
  padding: 50px 20px 20px;
  margin: 18px;
}

.color-palette__consultation-overlay__close-cta.close--icon {
  position: absolute;
  right: 10px;
  top: 10px;
  cursor: pointer;
}

.color-palette__consultation-overlay__text-content {
  text-align: center;
}

.color-palette__consultation-overlay__ctas {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.color-palette__consultation-overlay__ctas a {
  flex: 1 1 100%;
  width: 100%;
  margin: 10px;
  text-align: center;
  cursor: pointer;
}

.color-palette__consultation-overlay__ctas a:first-child {
  margin-left: 0;
}

.color-palette__consultation-overlay__ctas a:last-child {
  margin-right: 0;
}

.color-palette__consultation-overlay__content__container {
  display: grid;
  grid-template-columns: 1fr 1fr;
  grid-column-gap: 20px;
}

.color-palette__consultation-overlay__image {
  position: relative;
  height: 0;
  padding-top: 100%;
  width: 100%;
}

.color-palette__consultation-overlay__image img {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  object-fit: cover;
  object-position: center;
}

.color-palette--active .color-palette__consultation-overlay__text-content {
  text-align: left;
}

.color-palette__consultation-overlay__title {
  font-size: 24px;
  font-weight: 600;    
  border-bottom: 1px solid black;
  padding-bottom: 10px;
  margin-bottom: 20px;
}

.questionnaire_step_number_text {
  font-size: 24px;
  margin-bottom: 10px;
}

.selected-color-palette--category {
  font-size: 30px;
  font-weight: 600;
  margin-bottom: 15px;
}

.selected-color-palette--title {
  font-size: 36px;
  font-weight: 600;
}

.color-palette__consultation-overlay__text-content .button {
  display: block;
  text-align: center;
  width: 100%;
  margin-top: 20px;
}


